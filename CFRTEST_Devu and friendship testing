#include <iostream>
#include<set>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++)
	    {
	        cin>>a[i];
	    }
	    // not allowed duplicate in set 
	    set<int>s;
	    for(int i=0;i<n;i++)
	    {
	        s.insert(a[i]);
	    }
	    cout<<s.size()<<endl;
	}
	return 0;
}
